<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00000000">

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:alpha=".7"
        android:padding="10dp"
        android:background="#000000" />

    <RelativeLayout
        android:padding="15dp"
        android:layout_margin="15dp"
        android:id="@+id/linearLayout1"
        android:layout_width="310dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@color/bgcolor">

        <RelativeLayout
            android:layout_marginBottom="5dp"
            android:id="@+id/linearLayout_online"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical">

            <TextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:text="Value For Money :"
                android:textColor="@color/text_color_grey"
                android:textSize="15sp" />
            <RatingBar
                android:layout_alignParentRight="true"
                android:id="@+id/ratingbar_pricequality"
                android:rating="3"
                style="@style/StarRatingBar"
                android:layout_height="30dp"
                android:layout_gravity="right"
                android:layout_width="wrap_content" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_marginBottom="5dp"
            android:id="@+id/linearLayout_offline"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/linearLayout_online"
            android:gravity="center_vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:text="Service Quality :"
                android:textColor="@color/text_color_grey"
                android:textSize="15sp" />
            <RatingBar
                android:layout_alignParentRight="true"
                android:id="@+id/ratingbar_servicequality"
                android:rating="3"
                android:layout_gravity="right"
                style="@style/StarRatingBar"
                android:layout_height="30dp"
                android:layout_width="wrap_content" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_marginBottom="5dp"
            android:id="@+id/linearLayout_offline1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/linearLayout_offline"
            >

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:text="Punctuality:"
                android:textColor="@color/text_color_grey"
                android:textSize="15sp" />
            <RatingBar
                android:id="@+id/ratingbar_pickupquality"
                android:rating="3"
                android:layout_alignParentRight="true"
                android:layout_gravity="right"
                style="@style/StarRatingBar"
                android:layout_height="30dp"
                android:layout_width="wrap_content" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_marginBottom="5dp"
            android:id="@+id/linearLayout_offline2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/linearLayout_offline1"
            android:gravity="center_vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:text="Staff Professionalism :"
                android:textColor="@color/text_color_grey"
                android:textSize="15sp" />
            <RatingBar
                android:id="@+id/ratingbar_dropoffquality"
                android:rating="3"
                android:layout_alignParentRight="true"
                android:layout_gravity="right"
                style="@style/StarRatingBar"
                android:layout_height="30dp"
                android:layout_width="wrap_content" />
        </RelativeLayout>



